@using MVC_Project_BookStore.ViewModel
@model CategoryViewModel


@section Title{
    Category Details
}
@section subTitle{
   Details
}


<div class="card-body">

    <table class="table table-bordered">
        <thead>
            <tr>
                <th style="width: 10px">#</th>
                <th>Name</th>
                <th>Created On</th>
                <th>Updated On</th>
            </tr>
        </thead>
        <tbody>
           
                <tr>
                    <td>
                        @Model.Id
                    </td>
                    <td>
                    @Model.Name
                    </td>
                    <td>
                        @Model.CreatedOn
                    </td>
                <td>
                    @Model.UpdatedOn
                </td>
                </tr>
            
        </tbody>
    </table>
</div>