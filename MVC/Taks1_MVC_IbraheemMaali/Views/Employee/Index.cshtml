
@model List<Employee>
<h2>All Employee</h2>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Age</th>
            <th scope="col">Title</th>
       
        </tr>
    </thead>
    <tbody>

        @foreach (var employee in Model)
        {
        <tr>
                <th scope="row">@employee.Id</th>

                <td>@employee.Name</td>
                <td>@employee.Age</td>
                <td>@employee.Title</td>
        </tr>
        }
    </tbody>
</table>